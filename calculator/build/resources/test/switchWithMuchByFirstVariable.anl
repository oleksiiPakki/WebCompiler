m = pow(2,3); n = pow(3,2);

switch(m | n) {

    case 6: {
        m = pow(m ,2);
        n = pow(n, 2);
    }

    case 7: {
        m = m * 2;
        n = n * 2;
    }

    case 8: {
        m = m / 2;
        n = n / 3;
    }

    case 9: {
        m = m - 2;
        n = n - 2;
    }

    default: {
        print(m / 0, n / 0);
    }
}

print(m, n);